import java.util.*;

class WildcardDemo {

    public static void printList(List<?> list) {
        System.out.println("Printing elements:");
        for (Object obj : list) {
            System.out.println(obj);
        }
    }

    public static double sumOfList(List<? extends Number> list) {
        double sum = 0.0;
        for (Number num : list) {
            sum += num.doubleValue();
        }
        return sum;
    }

    public static void addNumbers(List<? super Integer> list) {
        list.add(10);
        list.add(20);
        list.add(30);
        System.out.println("Numbers added to list.");
    }

    public static void main(String[] args) {
        List<Integer> intList = Arrays.asList(1, 2, 3, 4, 5);
        List<Double> doubleList = Arrays.asList(2.5, 3.7, 1.2);
        List<Object> objList = new ArrayList<>();

        System.out.println("=== Unbounded Wildcard ===");
        printList(intList);

        System.out.println("\n=== Upper Bounded Wildcard ===");
        System.out.println("Sum of doubleList: " + sumOfList(doubleList));

        System.out.println("\n=== Lower Bounded Wildcard ===");
        addNumbers(objList);
        printList(objList);
    }
}
